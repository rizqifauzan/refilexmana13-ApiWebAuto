# Environment Configuration Example
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to repository

# API Authentication Token from gorest.co.in
# Get your token from: https://gorest.co.in/user
TOKEN=your_api_token_here

# Base URL for API Testing
BASE_URL=https://gorest.co.in

# Browser Configuration for Web Testing
BROWSER=chrome

# Headless mode (true/false) - Run browser in headless mode
HEADLESS_MODE=false

# Optional: Additional configurations
# TIMEOUT=10
# IMPLICIT_WAIT=5
# EXPLICIT_WAIT=10

